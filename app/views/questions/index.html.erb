<p style="color: green"><%= notice %></p>

<% content_for :title, "Questions" %>

<div><%= @quiz.title %></div>
<h1>Questions</h1>

<div id="questions">
  <% @questions.each do |question| %>
    <%= render question %>
    <p>
      <%= link_to "Show this question", [@quiz, question] %>
    </p>
  <% end %>
</div>

<%= link_to "New question", new_quiz_question_path(@quiz) %>
<%= link_to "Back to quiz", @quiz %>