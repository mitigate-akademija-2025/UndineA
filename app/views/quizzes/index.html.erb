<p style="color: green"><%= notice %></p>

<% content_for :title, "Quizzes" %>


<table id="quizzes">
  <thead>
    <tr>
      <th>Quizzes</th>
    </tr>
  </thead>
  <tbody>
    <% @quizzes.each do |quiz| %>
      <%= render 'quiz_row', quiz: quiz %>
    <% end %>
  </tbody>
</table>

<div><%= button_to "New quiz", new_quiz_path(@quiz), method: :get %></div>
